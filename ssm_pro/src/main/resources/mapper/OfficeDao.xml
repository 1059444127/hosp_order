<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sfturing.dao.OfficeDao">
	<!-- 通过ID地区信息 -->
	<select id="findOfficeByHosName" resultType="cn.sfturing.entity.Office" parameterType="java.lang.String">
		SELECT
		* FROM `hos_office`
		WHERE hospital_name = #{hospitalName}
	</select>
	

</mapper>